{
  "name": "enumish",
  "description": "Create Enum-like objects easily",
  "version": "1.0.0",
  "main": "./lib/index.js",
  "devDependencies": {
    "babel-cli": "^6.2.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.2.0",
    "mocha": "^2.2.5"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "build-test-es5": "npm run build && babel test --out-dir test-es5 && sed -i 's/\\/src\\//\\/lib\\//' test-es5/*.js",
    "watch": "babel src --out-dir lib --watch",
    "test": "mocha --ui tdd --compilers js:babel-register",
    "test-es5": "mocha --ui tdd test-es5",
    "prepublish": "npm run build"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "keywords": [
    "enum",
    "enums",
    "consts",
    "contants",
    "easy"
  ],
  "author": {
    "name": "Liad Yosef",
    "url": "https://github.com/liady"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/liady/enumish"
  }
}
